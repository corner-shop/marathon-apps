{
  "id": "/jenkins",
  "cmd": null,
  "cpus": 0.01,
  "mem": 2048,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/jenkins_home",
        "hostPath": "/data/sync-service/folders/jenkins/var-jenkins",
        "mode": "RW"
      },
      {
        "containerPath": "/var/log/nginx",
        "hostPath": "/data/sync-service/folders/jenkins/var-log-nginx",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "mesosphere/jenkins:3.5.0-2.107.2",
      "network": "HOST",
      "portMappings": [],
      "privileged": false,
      "parameters": [],
      "forcePullImage": true
    }
  },
  "env": {
    "SSH_KNOWN_HOSTS": "",
    "JENKINS_CONTEXT": "/service/jenkins",
    "JENKINS_SLAVE_AGENT_PORT": "50000",
    "JENKINS_AGENT_ROLE": "*",
    "JVM_OPTS": "-Xms1024m -Xmx1024m",
    "JENKINS_ROOT_URL": "http://jenkins.marathon.mesos/service/jenkins",
    "JENKINS_MESOS_MASTER": "zk://leader.mesos:2181/mesos",
    "JENKINS_AGENT_USER": "mesos",
    "JENKINS_OPTS": "",
    "JENKINS_FRAMEWORK_NAME": "jenkins"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 30,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "path": "/service/jenkins",
      "protocol": "HTTP",
      "ignoreHttp1xx": false
    }
  ],
  "labels": {
    "HAPROXY_0_REDIRECT_TO_HTTPS": "false",
    "HAPROXY_GROUP": "external",
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "HAPROXY_0_VHOST": "jenkins.marathon.mesos"
  },
  "portDefinitions": [
    {
      "port": 10005,
      "protocol": "tcp",
      "name": "nginx",
      "labels": {}
    },
    {
      "port": 10008,
      "protocol": "tcp",
      "name": "jenkins",
      "labels": {}
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 0
  }
}
