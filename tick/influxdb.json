{
  "id": "/tick/influxdb",
  "cmd": null,
  "cpus": 0.01,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "/var/lib/influxdb",
        "hostPath": "/data/sync-service/folders/tick/influxdb",
        "mode": "RW"
      }
    ],
    "docker": {
      "image": "tutum/influxdb",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 8086,
          "hostPort": 0,
          "servicePort": 10005,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 8083,
          "hostPort": 0,
          "servicePort": 10018,
          "protocol": "tcp",
          "labels": {}
        },
        {
          "containerPort": 2003,
          "hostPort": 31006,
          "servicePort": 10020,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "PRE_CREATE_DB": "cadvisor;graphite",
    "GRAPHITE_DB": "graphite"
  },
  "labels": {
    "HAPROXY_0_GROUP": "external",
    "HAPROXY_1_GROUP": "external",
    "HAPROXY_1_VHOST": "influxdb-admin.service.tinc-core-vpn",
    "HAPROXY_0_VHOST": "influxdb-api.service.tinc-core-vpn"
  },
  "portDefinitions": [
    {
      "port": 10005,
      "protocol": "tcp",
      "name": "default",
      "labels": {}
    },
    {
      "port": 10018,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 10020,
      "protocol": "tcp",
      "labels": {}
    },
    {
      "port": 0,
      "protocol": "tcp",
      "labels": {}
    }
  ]
}